<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Pane?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.kognitywistyka.ppa2021.FANATYCYController">
   <top>
      <Pane prefHeight="157.0" prefWidth="800.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="chooseButton" layoutX="21.0" layoutY="36.0" mnemonicParsing="false" onAction="#loadData" prefHeight="31.0" prefWidth="177.0" text="Załaduj wstępne dane" />
            <Button fx:id="fanatyk" disable="true" layoutX="240.0" layoutY="36.0" mnemonicParsing="false" onAction="#addfanatyk" prefHeight="31.0" prefWidth="160.0" text="Dodaj fanatyka" />
            <Button fx:id="ryba" layoutX="476.0" layoutY="30.0" mnemonicParsing="false" onAction="#addryba" prefHeight="31.0" prefWidth="197.0" text="Dodaj złowioną rybę" disable="true" />
         </children>
      </Pane>
   </top>
   <center>
      <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
